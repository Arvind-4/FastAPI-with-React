{
  "name": "ui",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "deploy": "pm2 serve build/ 3000 --name 'react-build' --spa --no-daemon",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md,html}\""
  },
  "dependencies": {
    "@carbon/react": "1.1.0",
    "pm2": "^5.3.0",
    "react": "16.10.0",
    "react-dom": "16.10.0",
    "react-router-dom": "5.0.0",
    "sass": "1.51.0",
    "serve": "^14.2.1"
  },
  "devDependencies": {
    "@commitlint/cli": "7.5.2",
    "@commitlint/config-conventional": "7.5.0",
    "@types/node": "^20.8.6",
    "@types/react": "^18.2.24",
    "@types/react-dom": "^18.2.8",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^4.1.0",
    "@vitejs/plugin-react-swc": "^3.4.0",
    "and": "^0.0.3",
    "husky": "1.3.1",
    "lint-staged": "8.1.5",
    "prettier": "1.17.0",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vite-plugin-svgr": "^4.1.0",
    "vite-tsconfig-paths": "^4.2.1",
    "wait-for-expect": "3.0.2"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "prettier": {
    "jsxBracketSameLine": true,
    "printWidth": 80,
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
